{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "exclude": [
    "./src/blockchain/abi"
  ],
   "include": [
    "src/**/*"
  ]
}
